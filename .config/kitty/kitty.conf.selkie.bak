font_family      JetBrainsMono Nerd Font Mono
bold_font        auto
italic_font      auto
bold_italic_font auto
font_size        16
symbol_map       codepoints Font Awesome 5 Free Regular
symbol_map       codepoints Noto Color Emoji
symbol_map       codepoints PowerlineSymbols
symbol_map       codepoints Font Awesome 5 Brands Regular
symbol_map       codepoints FontAwesome
#: disable_ligatures always

cursor #FFFFE1
cursor_text_color #2d202e

foreground #FFFFE1
background #2d202e
background_opacity 0.40

selection_foreground #2d202e
selection_background #4fb6dd

color0 #3d3a40  
color8 #504d53

#: black

color1 #e23f5c
color9 #fc4767

#: red

color2  #3dcb69
color10 #44e275

#: green

color3  #e0cf61
color11 #ffea70

#: yellow

color4  #5267d0
color12 #5c73e8

#: blue

color5  #da5988
color13 #f36398

#: magenta

color6  #47a3c6
color14 #4fb6dd

#: cyan

color7  #e5e5ca
color15 #ffffe1

#: white

shell zsh
editor nvim

scrollback_lines 2000

url_color #47a3c6
url_style curly
open_url_modifiers kitty_mod
open_url_with default
url_prefixes http https file ftp

copy_on_select no
strip_trailing_spaces never

terminal_select_modifiers shift

focus_follows_mouse no

repaint_delay 10
sync_to_monitor yes

enable_audio_bell no

remember_window_size no

active_border_color #3dcb69
inactive_border_color #e23f5c
bell_border_color #da5988

tab_bar_edge bottom
tab_separator " â”‡"
